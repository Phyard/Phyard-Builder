<?xml version="1.0" encoding="utf-8"?>
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml" xmlns="editor.dialog.*"
    title="Variables Setting" width="500" height="500"
    borderAlpha="0.77"
    >

   <mx:VBox label="Custom Entity Properties" borderStyle="solid" height="100%" width="100%"
            paddingTop="5" paddingLeft="5" paddingRight="5" paddingBottom="5"
            horizontalScrollPolicy="off" verticalScrollPolicy="off"
            horizontalGap="0" verticalGap="0" borderThickness="0">
      <VariablesEditPanel id="customEntityPropertiesEditPanel" width="100%" height="100%"/>
   </mx:VBox>

   <mx:HBox width="100%">
      <mx:Spacer width="100%"/>
      <mx:Button label="Close" click="OnClickCloseButton ();"/>
      <mx:Spacer width="100%"/>
   </mx:HBox>

   <mx:Script>
        <![CDATA[
         
         import mx.managers.PopUpManager;
         
         import editor.runtime.Runtime;
         
         private var OnClose:Function = null;
         
         private function OnInitPanel ():void
         {
         }
         
         public function SetCloseFunc (onClose:Function):void
         {
            OnClose = onClose;
         }
         
         private function OnClickCloseButton():void 
         {
             PopUpManager.removePopUp(this);
             
             if (OnClose != null)
               OnClose ();
         }
         
       ]]>
   </mx:Script>
   
</mx:TitleWindow>